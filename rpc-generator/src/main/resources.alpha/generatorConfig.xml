<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <properties url="file:E:/github/rpc-parent/rpc-generator/src/main/resources.alpha/mbg.properties"/>
    <classPathEntry location="${mbg.jar.path}"/>
    <context id="context1" defaultModelType="flat">

        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/rpc-web?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=false&amp;nullNamePatternMatchesAll=true"
                        userId="root"
                        password="123456"/>

        <javaModelGenerator targetPackage="com.rayvision.rpc.web.entity"
                            targetProject="${target.module.sdk.base}/src/main/java">
            <property name="mergeable" value="false"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="${target.module.sdk.base}/src/main/resources"/>

        <javaClientGenerator targetPackage="com.rayvision.rpc.web.mapper"
                             targetProject="${target.module.sdk.base}/src/main/java"
                             type="XMLMAPPER"/>

        <table schema="rendering_new" tableName="tb_user_account" domainObjectName="UserAccount"></table>

    </context>
</generatorConfiguration>